  },
  "dependencies": {
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.12.5",
    "@types/he": "^1.1.1",
    "axios": "^0.21.1",
    "gatsby-source-filesystem": "^3.8.0",
    "lodash": "^4.17.21",
    "rehype-parse": "^7.0.1",
    "rehype-remark": "^8.1.1",
